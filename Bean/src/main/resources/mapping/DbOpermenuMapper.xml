  <insert id="insert" parameterType="com.bean.model.DbOpermenu">
    insert into db_opermenu (OPER, MENUID)
    values (#{oper,jdbcType=VARCHAR}, #{menuid,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.bean.model.DbOpermenu">
    insert into db_opermenu
