<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bean.dao.EmpMapper">
  <resultMap id="BaseResultMap" type="com.bean.model.Emp">
    <result column="EID" jdbcType="VARCHAR" property="eid" />
    <result column="CITY" jdbcType="VARCHAR" property="city" />
    <result column="ENAME" jdbcType="VARCHAR" property="ename" />
    <result column="BDATE" jdbcType="TIMESTAMP" property="bdate" />
    <result column="STATUS" jdbcType="VARCHAR" property="status" />
    <result column="SEX" jdbcType="VARCHAR" property="sex" />
  </resultMap>
  <insert id="insert" parameterType="com.bean.model.Emp">
    insert into emp (EID, CITY, ENAME, 
      BDATE, 'STATUS', SEX
      )
    values (#{eid,jdbcType=VARCHAR}, #{city,jdbcType=VARCHAR}, #{ename,jdbcType=VARCHAR}, 
      #{bdate,jdbcType=TIMESTAMP}, #{status,jdbcType=VARCHAR}, #{sex,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.bean.model.Emp">
    insert into emp
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="eid != null">
        EID,
      </if>
      <if test="city != null">
        CITY,
      </if>
      <if test="ename != null">
        ENAME,
      </if>
      <if test="bdate != null">
        BDATE,
      </if>
      <if test="status != null">
        'STATUS',
      </if>
      <if test="sex != null">
        SEX,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="eid != null">
        #{eid,jdbcType=VARCHAR},
      </if>
      <if test="city != null">
        #{city,jdbcType=VARCHAR},
      </if>
      <if test="ename != null">
        #{ename,jdbcType=VARCHAR},
      </if>
      <if test="bdate != null">
        #{bdate,jdbcType=TIMESTAMP},
      </if>
      <if test="status != null">
        #{status,jdbcType=VARCHAR},
      </if>
      <if test="sex != null">
        #{sex,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>