<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bean.dao.DeptMapper">
  <resultMap id="BaseResultMap" type="com.bean.model.Dept">
    <result column="DID" jdbcType="VARCHAR" property="did" />
    <result column="DNAME" jdbcType="VARCHAR" property="dname" />
    <result column="DCITY" jdbcType="VARCHAR" property="dcity" />
  </resultMap>
  <insert id="insert" parameterType="com.bean.model.Dept">
    insert into dept (DID, DNAME, DCITY
      )
    values (#{did,jdbcType=VARCHAR}, #{dname,jdbcType=VARCHAR}, #{dcity,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.bean.model.Dept">
    insert into dept
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="did != null">
        DID,
      </if>
      <if test="dname != null">
        DNAME,
      </if>
      <if test="dcity != null">
        DCITY,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="did != null">
        #{did,jdbcType=VARCHAR},
      </if>
      <if test="dname != null">
        #{dname,jdbcType=VARCHAR},
      </if>
      <if test="dcity != null">
        #{dcity,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>